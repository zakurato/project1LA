
@model project1LA.Models.DTO.UserDTO
@{
    ViewBag.Title = "Create";
}

<h2>Create new user</h2>

<h2> Usual way</h2>
<div>
    <form action="/User/Create" method="post" class="form">
        <div class="form-group">
